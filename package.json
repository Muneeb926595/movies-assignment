{
  "name": "UpTodo",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":20,\"functions\":15,\"lines\":20,\"statements\":20}}'",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=e2e",
    "test:component": "jest --testPathPattern=__tests__/components",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "detox test",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "test:update-snapshots": "jest -u",
    "test:clear-cache": "jest --clearCache",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "start": "react-native start",
    "start:rozenite": "WITH_ROZENITE=true react-native start",
    "release:prepare": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.13.0",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.8.6",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.2",
    "flat": "^6.0.1",
    "i18next": "^25.7.3",
    "react": "19.2.0",
    "react-i18next": "^16.5.0",
    "react-native": "0.83.0",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-get-random-values": "^2.0.0",
    "react-native-mmkv": "^4.1.0",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-pager-view": "^7.0.2",
    "react-native-reanimated": "^4.2.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.19.0",
    "react-native-splash-view": "^0.0.19",
    "react-native-svg": "^15.15.1",
    "react-native-theme-switch-animation": "^0.8.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "^0.7.1",
    "react-native-youtube-iframe": "^2.4.1",
    "sonner-native": "^0.22.2",
    "styled-components": "^6.2.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@react-native/metro-config": "0.83.0",
    "@react-native/typescript-config": "0.83.0",
    "@rozenite/metro": "^1.1.0",
    "@rozenite/mmkv-plugin": "^1.1.0",
    "@rozenite/network-activity-plugin": "^1.1.0",
    "@rozenite/react-navigation-plugin": "^1.1.0",
    "@rozenite/redux-devtools-plugin": "^1.1.0",
    "@rozenite/require-profiler-plugin": "^1.1.0",
    "@rozenite/tanstack-query-plugin": "^1.1.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.21",
    "@types/react": "^19.2.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "@types/styled-components": "^5.1.36",
    "@types/styled-components-react-native": "^5.2.5",
    "@testing-library/react-native": "^12.8.1",
    "@testing-library/jest-native": "^5.4.3",
    "detox": "^20.14.7",
    "eslint": "^8.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.3",
    "jest-circus": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
